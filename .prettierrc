{
	"printWidth": 120,
	"tabWidth": 2,
	"semi": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "avoid",
	"htmlWhitespaceSensitivity": "css",
	"endOfLine": "lf",
	"proseWrap": "always",
	"useTabs": true,
	"singleQuote": true,
	"plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
